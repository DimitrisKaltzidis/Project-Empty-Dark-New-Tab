{
	"manifest_version": 2,
	"name": "Empty Dark New Tab",
	"description": "Replaces the new tab with an empty dark one",
	"version": "1.1",
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"browser_action": {
		"default_icon": "logo.png"
	},
	"icons": {
		"16": "logo.png",
		"48": "logo.png",
		"128": "logo.png"
	},
	"permissions": ["tabs"],
	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	}
}